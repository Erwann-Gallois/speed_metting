    <main>
        {% set route = app.request.get('_route')%}
        {% block header %}{% endblock %}
        <nav class="navbar navbar-expand-lg custom-navbar">
            <div class="container">
                {% if is_granted("ROLE_ORGANISATEUR") %}
                    <a class="navbar-brand" href="{{path ('admin')}}">
                        Admin
                    </a>
                {% else %}
                    <a class="navbar-brand" href="{{path ('accueil')}}">
                        Speed Meeting EvryBio 2024
                    </a>
                {% endif %}
                {% if app.user %}
                        <a href="{{path ('deconnexion')}}" class="btn custom-btn d-lg-none ms-auto me-4">Deconnexion</a>
                {% else %}
                    <a href="{{path ('connexion')}}" class="btn custom-btn d-lg-none ms-auto me-4">Connexion</a>
                {% endif %}
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
