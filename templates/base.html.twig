                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav align-items-lg-center ms-auto me-lg-5">
                        <li class="nav-item">
                            <a class="nav-link-class click-scroll" href="">Présentation</a>
                        </li>
                        {# <li class="nav-item">
                            <a class="nav-link click-scroll" href="">Professionnels</a>
                        </li> #}
                        <li class="nav-item">
                            {% if route != "accueil" %}
                                <a class="nav-link-class" href="{{path ('organisation')}}">Organisation</a>
                            {% else %}
                                <a class="nav-link-class click-scroll" href="">Organisation</a>
                            {% endif %}
                        </li>
                        <li class="nav-item">
                            {% if route != "accueil" %}
                                <a class="nav-link-class" href="{{path ('contact')}}">Contact</a>
                            {% else %}
                                <a class="nav-link-class click-scroll" href="">Contact</a>
                            {% endif %}
                        </li>
                        <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Liste
                                </a>
                                <ul class="dropdown-menu">
                                    <li><a class="dropdown-item" href="{{path ('liste_pro')}}">Professionnels</a></li>
                                    <li><a class="dropdown-item" href="{{path ('front_liste_eleve')}}">Eleves</a></li>
                                </ul>
                            </li>
                        {% if app.user %}
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Mon compte
                                </a>
                                <ul class="dropdown-menu">
                                    <li><a class="dropdown-item" href="{{path ('compte')}}">Profil</a></li>
                                    <li><a class="dropdown-item" href="{{path ('planning')}}">Planning</a></li>
                                    {% if afficherLien %}
                                        <li><a class="dropdown-item" href="{{path ('reservation')}}">Reservation</a></li>
                                    {% endif %}
                                </ul>
                            </li>
                        {% endif %}
                    </ul>
                    {% if app.user %}
                        <a href="{{path ('deconnexion')}}" class="btn custom-btn d-lg-block d-none">Deconnexion</a>
                    {% else %}
                        <a href="{{path ('connexion')}}" class="btn custom-btn d-lg-block d-none">Connexion</a>
                    {% endif %}
                    
                </div>
            </div>
        </nav>
